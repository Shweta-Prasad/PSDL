//Memory Transfer

#include<stdio.h>
#include<pic18f4550.h>

void main(void)
{
    int source[]={0x24,0x25,0x26};
    int dest[]={0x34,0x35,0x36};
    
    for(int i=0; i<3; i++){
        source[i]=dest[i];
    }
}

*******************************
//Memory Exchange

#include<stdio.h>
#include<pic18f4550.h>

void main(void)
{
    int temp;
    int source[]={0x24,0x25,0x26};
    int dest[]={0x34,0x35,0x36};
    
    for(int i=0; i<3; i++){
        temp=source[i];
        source[i]=dest[i];
        dest[i]=temp;
    }
}
